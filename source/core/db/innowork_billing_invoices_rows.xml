<?xml version='1.0'?>
<database>

    <table name="innowork_billing_invoices_rows">
        <field	name="id"   type="integer"  notnull="1"/>
        <field  name="invoiceid"  type="integer"    notnull="1"/>
        <field  name="description"  type="text"/>
        <field  name="amount"   type="decimal" length="10,2"/>
        <field  name="vatid"  type="integer"/>
        <field  name="quantity"  type="decimal" length="10,2" default="0"/>
        <field  name="discount"  type="integer" default="0"/>
        <field  name="rowcounter"   type="integer"/>

        <key    field="id"  type="unique"/>
    </table>

    <sequence name="innowork_billing_invoices_rows_id_seq"/>

</database>
